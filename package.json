    "gulp-clean-css": "^4.3.0",
    "gulp-postcss": "^10.0.0",
    "gulp-remember": "^1.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-sass-partials-imported": "^1.0.7",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-terser": "^2.1.0",
    "node-sass": "^9.0.0",
    "plugin-error": "^2.0.1"
