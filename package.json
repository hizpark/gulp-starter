{
  "name": "gulp-starter",
  "version": "1.0.0",
  "description": "A development toolkit that leverages Gulp.js to streamline Bootstrap-based web development",
  "keywords": [
    "gulp",
    "builder",
    "bootstrap"
  ],
  "license": "MIT",
  "author": "Harper Jang <harper.jang@outlook.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hizpark/gulp-starter.git"
  },
  "scripts": {
